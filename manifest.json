{
  "manifest_version": 2,
  "name": "__MSG_extName__",
  "version": "1.0.0",
  "default_locale" : "en",
  "description": "__MSG_extDescription__",
  "icons": {
    "16": "img/icon.png",
    "48": "img/icon.png",
    "128": "img/icon.png"
